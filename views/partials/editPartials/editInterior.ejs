<div id="interiorData">

<!-- show only 'groundFloor' 'belfry' and 'landing' -->
	<% for (var key in tower.val()) { %>
		<% if (tower.val().hasOwnProperty(key)) { %>
			<% if (key.substring(0, 7) == "landing" || key == "belfry" || key == "groundFloor") { %> 
				<div class="ui fluid accordion">
					<div class="title">
						<h2><%= key %></h2>
					</div>
					<div class="content whitePaddedContent">
					</div>

					<div class="title">
						<i class="dropdown icon"></i>
						data
					</div>
					<div class="content whitePaddedContent">
						<% for (var field in tower.val()[key]) { %>
							<% if (tower.val()[key].hasOwnProperty(field)) { %>
								<input type="text" name="<%= key %>[<%= tower.val()[key][field] %>]" placeholder="<%= field %>" value="<%= tower.val()[key][field] %>"  >
							<% } %> 
						<% } %>
					</div>

					<div class="title">
						<p>submit</p>
					</div>
					<div class="content whitePaddedContent">
						<p>be safe</p>
					</div>
				</div>
			<% } %> 
		<% } %>
	<% } %>

</div>